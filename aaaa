-- Setup
local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Create GUI
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "FakeLoadingUI"
screenGui.ResetOnSpawn = false
screenGui.Parent = playerGui

-- Main Frame
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0.5, 0, 0.25, 0)
frame.Position = UDim2.new(0.25, 0, 0.35, 0)
frame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
frame.BorderSizePixel = 0
frame.Parent = screenGui

-- Loading Bar Background
local barBG = Instance.new("Frame")
barBG.Size = UDim2.new(0.9, 0, 0.2, 0)
barBG.Position = UDim2.new(0.05, 0, 0.5, 0)
barBG.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
barBG.BorderSizePixel = 0
barBG.Parent = frame

-- Progress Bar
local bar = Instance.new("Frame")
bar.Size = UDim2.new(0, 0, 1, 0)
bar.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
bar.BorderSizePixel = 0
bar.Parent = barBG

-- Percentage Text
local percentLabel = Instance.new("TextLabel")
percentLabel.Size = UDim2.new(1, 0, 0.25, 0)
percentLabel.Position = UDim2.new(0, 0, 0.1, 0)
percentLabel.BackgroundTransparency = 1
percentLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
percentLabel.Font = Enum.Font.SourceSansBold
percentLabel.TextScaled = true
percentLabel.Text = "0%"
percentLabel.Parent = frame

-- Status Text (Under the bar)
local statusLabel = Instance.new("TextLabel")
statusLabel.Size = UDim2.new(1, 0, 0.25, 0)
statusLabel.Position = UDim2.new(0, 0, 0.8, 0)
statusLabel.BackgroundTransparency = 1
statusLabel.TextColor3 = Color3.fromRGB(180, 180, 180)
statusLabel.Font = Enum.Font.SourceSans
statusLabel.TextScaled = true
statusLabel.Text = "Initializing..."
statusLabel.Parent = frame

-- Fake status messages
local messages = {
	"Bypassing anti-cheat...",
	"Copying your pets...",
	"Checking if script is online...",
	"Injecting virus.exe...",
	"Disabling firewall...",
	"Gathering account data...",
	"Verifying ownership...",
	"Analyzing player inventory...",
	"Loading backdoors...",
	"Installing keylogger..."
}

-- Animate loading
local progress = 0
while progress <= 100 do
	percentLabel.Text = progress .. "%"
	bar.Size = UDim2.new(progress / 100, 0, 1, 0)
	if progress % 10 == 0 then
		statusLabel.Text = messages[math.random(1, #messages)]
	end
	progress += 1
	wait(0.05)
end

-- Remove UI, leave player alone
screenGui:Destroy()
